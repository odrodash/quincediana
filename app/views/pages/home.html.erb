          <script>
            mapboxgl.accessToken = 'pk.eyJ1Ijoib2Ryb2Rhc2giLCJhIjoiY2sxbWhpeTQwMDB1MzNjcWRjNm96MDQ5dSJ9.TtOoR4cri7_mcW-93HJ7Jw';
            var map = new mapboxgl.Map({
              container: 'map',
              style: 'mapbox://styles/odrodash/ck7lghlso0fye1ilhg0le43zv',
              center: [-63.1341, -17.8006],
              zoom: 11
            });

            map.on("load", function () {
            /* Image: An image is loaded and added to the map. */
              map.loadImage("https://res.cloudinary.com/dmuryw7rj/image/upload/v1583853429/quincediana/location4_qrdujg.png", function(error, image) {
                  if (error) throw error;
                  map.addImage("custom-marker", image);
                  /* Style layer: A style layer ties together the source and image and specifies how they are displayed on the map. */
                  map.addLayer({
                    id: "markers",
                    type: "symbol",
                    /* Source: A data source specifies the geographic coordinate where the image marker gets placed. */
                    source: {
                      type: "geojson",
                      data: { type: 'FeatureCollection',
                            features: [{
                              type: 'Feature',
                              properties: {},
                              geometry: { type: "Point", coordinates: [-63.1341, -17.8006] }
                            }]
                      }
                    },
                    layout: {
                      "icon-image": "custom-marker",
                    }
                  });
              });
            });
            map.addControl(new mapboxgl.NavigationControl());
          </script>
